{
  "name": "release-helper",
  "description": "Release helper cli to help/assist with proper release flow.",
  "version": "0.0.0",
  "author": "Eduardo Azevedo Grigolo",
  "bin": {
    "relper": "./bin/run"
  },
  "bugs": "https://github.com/scripts/release-helper/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.0",
    "listr": "^0.14.3",
    "module-alias": "2.2.2",
    "ora": "^5.1.0",
    "rxjs": "^6.6.3",
    "tslib": "2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.24.1",
    "@oclif/test": "1.2.7",
    "@types/chai": "4.2.14",
    "@types/inquirer": "7.3.1",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "cli-ux": "5.5.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.2.0",
    "eslint-plugin-prettier": "3.1.4",
    "globby": "11.0.1",
    "inquirer": "7.3.3",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "ts-node": "8.10.2",
    "typescript": "4.1.2",
    "prettier": "2.2.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://git.frontiercargroup.org/tools/release-helper-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "relper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://git.frontiercargroup.org/tools/release-helper-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "_moduleAliases": {
    "@helpers": "src/helpers"
  }
}
